# Personal Websites

Host bonniemooredesigns.com
    User bonniemooredesigns

Host incrementalism.net
    User incrementalism
    ForwardAgent yes

# Stuff for work

Host *.amazonaws.com
    IdentityFile ~/.ec2/elasticbamboo-dogfood/elasticbamboo.pem
    User root

Host bitbucket.org
    Compression yes
    User hg

# Unicorn Contegix
# Manager nodes
Host manager-0.uc-1.atlassian.com m0uc1
    Hostname manager-0.uc-1.atlassian.com
    ForwardAgent yes
Host manager-1.uc-1.atlassian.com m1uc1
    Hostname manager-1.uc-1.atlassian.com
    ForwardAgent yes
# Jump Boxes
Host jump1.uc-1.atlassian.com jump1 uprod
    Hostname jump1.uc-1.atlassian.com
    ForwardAgent yes
Host jump2.uc-1.atlassian.com jump2
    Hostname jump2.uc-1.atlassian.com
    ForwardAgent yes

# Sydney GlobalSwitch
Host jump1.syd-gs.atlassian.com sydjump1 udev
    Hostname jump1.syd-gs.atlassian.com
    ForwardAgent yes
Host jump2.syd-gs.atlassian.com sydjump2
    Hostname jump2.syd-gs.atlassian.com
    ForwardAgent yes
Host manager-0.syd-gs.atlassian.com m0syd
    Hostname manager-0.syd-gs.atlassian.com

# Unicorn instances
Host *.jira.com *.atlassian.net
    User root
    ProxyCommand ssh -T uprod nc %h.internal %p
Host *.jira-dev.com
    User root
    ProxyCommand ssh -T udev nc %h.internal %p

# your own dev instance on Unicorn
Host unicorn
    Hostname $USER.jira-dev.com
    User root
    ProxyCommand ssh -T udev nc %h.internal %p
