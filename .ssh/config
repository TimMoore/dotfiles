# Personal Websites

Host bonniemooredesigns.com
    User bonniemooredesigns

Host incrementalism.net
    User incrementalism
    ForwardAgent yes

# Stuff for work

Host *.amazonaws.com
    IdentityFile ~/.ec2/elasticbamboo-dogfood/elasticbamboo.pem
    User root

Host bitbucket.org
    Compression yes
    User hg

# Unicorn Contegix
# Manager nodes
Host manager-0.uc-1.atlassian.com m0uc1
    Hostname manager-0.uc-1.atlassian.com
    ForwardAgent yes
Host manager-1.uc-1.atlassian.com m1uc1
    Hostname manager-0.uc-1.atlassian.com
    ForwardAgent yes
# jumpboxes
Host jump1.uc-1.atlassian.com uprod udev #udev is temporary here until syd-gs is back together
    Hostname jump1.uc-1.atlassian.com
    ForwardAgent yes
Host jump2.uc-1.atlassian.com
    Hostname jump2.uc-1.atlassian.com
    ForwardAgent yes

# Sydney GlobalSwitch
Host manager-0.syd-gs.atlassian.com m0syd #udev
    Hostname manager-0.syd-gs.atlassian.com
    ForwardAgent yes
Host manager-1.syd-gs.atlassian.com m1syd
    Hostname manager-0.syd-gs.atlassian.com
    ForwardAgent yes

# Unicorn instances
Host sdog.jira.com sdog
    User root
    ProxyCommand ssh -T uprod nc sdog.jira.com.internal %p
Host *.jira.com.internal
    User root
    ProxyCommand ssh -T uprod nc %h %p
    #StrictHostKeyChecking no
Host *.jira-dev.com
    User root
    ProxyCommand ssh -T udev nc %h.internal %p
    #StrictHostKeyChecking no
Host syd-gs-* # don't use the FQDN otherwise it'll match above lines. ProxyCommand adds it for you.
    User root
    ProxyCommand ssh -T m0syd nc %h.jira.com.internal %p
    #StrictHostKeyChecking no

# your own dev instance on Unicorn
Host unicorn
    Hostname $USER.jira-dev.com
    User root
    ProxyCommand ssh -T udev nc %h.internal %p
    #StrictHostKeyChecking no
